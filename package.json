{
  "name": "personal-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss -i styles/globals.css -o styles/dist.css -w\"",
    "build": "tailwindcss -m -i styles/globals.css -o styles/dist.css && next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "node ./contentful/setup.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "@next/font": "^13.0.6",
    "@react-three/drei": "^9.46.5",
    "@react-three/fiber": "^8.9.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "contentful-import": "^8.5.18",
    "cross-env": "^7.0.3",
    "framer-motion-3d": "^7.10.0",
    "next": "^13.0.8-canary.0",
    "next-transpile-modules": "^10.0.0",
    "postcss": "^8.4.20",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spinners": "^0.13.7",
    "server-only": "^0.0.1",
    "tailwindcss": "^3.2.4",
    "three": "^0.147.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/three": "^0.146.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "^13.0.7",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-jsx": "^11.0.0",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "prettier": "2.8.1",
    "standard": "^17.0.0",
    "typescript": "^4.9.4"
  }
}
